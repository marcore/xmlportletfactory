<?xml version="1.0" encoding="UTF-8"?>
<definition xmlns="http://www.xmlportletfactory.org/xml/xmlportletfactory" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xmlportletfactory.org/xml/xmlportletfactory xmlportletfactory.xsd">
	<commonData>
        <client>alexiglesia</client>
		<projectName>SampleG</projectName>
        <version>2</version>
	</commonData>
	<applications>
		<application>
			<classDef>
				<name>Users</name>
				<title>Mantenimiento de Users</title>
				<mode>
					<add>true</add>
					<delete>true</delete>
					<modify>true</modify>
					<browse>true</browse>
				</mode>
			</classDef>
			<fileDef>
				<name>users</name>
				<fields>
					<field>
						<name>usersId</name>
						<title>Id</title>
						<type>
							<long>
								<length>5</length>
								<signed>false</signed>
								<nullable>false</nullable>
								<order>true</order>
							</long>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>usersName</name>
						<title>Nombre</title>
						<type>
							<varchar>
								<length>40</length>
								<order>true</order>
							</varchar>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>usersTlfo</name>
						<title>Telefono</title>
						<type>
							<varchar>
								<length>40</length>
								<order>true</order>
							</varchar>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>false</required>
					</field>
					<field>
						<name>usersMail</name>
						<title>e-mail</title>
						<type>
							<varchar>
								<length>40</length>
								<regexp>email</regexp>
								<order>true</order>
							</varchar>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>usersTexto</name>
						<title>Texto</title>
						<type>
							<varchar>
								<length>40</length>
								<order>true</order>
							</varchar>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>false</required>
					</field>
				</fields>
			</fileDef>
		</application>
	</applications>
</definition>

