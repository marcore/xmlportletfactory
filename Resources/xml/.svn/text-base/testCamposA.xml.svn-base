<?xml version="1.0" encoding="UTF-8"?>
<definition xmlns="http://www.xmlportletfactory.org/xml/xmlportletfactory" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xmlportletfactory.org/xml/xmlportletfactory xmlportletfactory.xsd">
	<commonData>
        <client>Activa</client>
		<projectName>testCamposA</projectName>
        <version>1</version>
	</commonData>
	<applications>
		<application>
			<classDef>
				<name>testCamposA</name>
				<title>Mantenimiento de testCampos</title>
				<mode>
					<add>true</add>
					<delete>true</delete>
					<modify>true</modify>
					<browse>true</browse>
				</mode>
			</classDef>
			<fileDef>
				<name>test</name>
				<fields>
					<field>
						<name>testId</name>
						<title>Codigo</title>
						<type>
							<long>
								<length>5</length>								
								<signed>false</signed>
								<nullable>false</nullable>
							</long>
						</type>
						<showFieldInView>true</showFieldInView>						
						<required>true</required>
					</field>
					<field>
						<name>testInt</name>
						<title>testInt</title>
						<type>
							<int>
								<length>5</length>
								<signed>false</signed>
								<zerofill>false</zerofill>
								<nullable>false</nullable>
							</int>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>testLong</name>
						<title>testLong</title>
						<type>
							<long>
								<length>5</length>
								<signed>false</signed>
								<nullable>false</nullable>
							</long>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>testNumeric</name>
						<title>testNumeric</title>
						<type>
							<numeric>
								<length>5</length>
								<decimals>2</decimals>								
							</numeric>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>testFloat</name>
						<title>testFloat</title>
						<type>
							<numeric>
								<length>5</length>
								<decimals>2</decimals>								
							</numeric>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>testDate</name>
						<title>testDate</title>
						<type>
							<date></date>
						</type>
						<showFieldInView>true</showFieldInView>
					</field>
					<field>
						<name>testDatetime</name>
						<title>testDatetime</title>
						<type>
							<datetime></datetime>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>					
					<field>
						<name>testEnum</name>
						<title>testEnum</title>
						<type>
							<enum>
								<values>'NO','SI'</values>
								<default>'NO'</default>
							</enum>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>testVarchar1</name>
						<title>Correo electronico</title>
						<type>
							<varchar>
								<length>40</length>
								<regexp>email</regexp>
							</varchar>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					<field>
						<name>testVarchar2</name>
						<title>Codigo postal</title>
						<type>
							<varchar>
								<length>40</length>
								<regexp>zipspain</regexp>
							</varchar>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					
					<field>
						<name>testVarchar3</name>
						<title>Pagina web</title>
						<type>
							<varchar>
								<length>40</length>
								<regexp>url</regexp>
							</varchar>
						</type>
						<showFieldInView>true</showFieldInView>
						<required>true</required>
					</field>
					
					<field>
						<name>testText</name>
						<title>testText</title>
						<type>
							<text></text>
						</type>
						<showFieldInView>true</showFieldInView>					
						<required>true</required>
					</field>					
					<field>
						<name>testImage</name>
						<title>testImage</title>
						<type>
							<image></image>
						</type>
						<showFieldInView>true</showFieldInView>					
						<required>true</required>
					</field>
					<field>
						<name>testDocument</name>
						<title>testDocument</title>
						<type>
							<document></document>
						</type>
						<showFieldInView>true</showFieldInView>					
						<required>true</required>
					</field>									
				</fields>
			</fileDef>
		</application>
	</applications>
</definition>
	
	